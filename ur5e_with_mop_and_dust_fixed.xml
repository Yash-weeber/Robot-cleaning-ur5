    <mesh file="wrist1_2.obj"/>
    <mesh file="wrist2_0.obj"/>
    <mesh file="wrist2_1.obj"/>
    <mesh file="wrist2_2.obj"/>
    <mesh file="wrist3.obj"/>

    <mesh file="test.stl" scale="0.28 0.29 0.1" name="table"/>
    <mesh name="base_mesh" file="mop_base.obj" scale="0.002 0.002 0.002"/>
    <mesh name="handle_mesh" file="mop_handle.obj" scale="0.002 0.002 0.002"/>
    <mesh name="dust_ball_mesh" file="simple_small_ball.stl" scale="0.005 0.005 0.005"/>
  </asset>

