<?xml version="1.0" encoding="UTF-8"?>
<mujoco model="ur5e_with_mop_and_dust">
  <compiler angle="radian" meshdir="assets" autolimits="true"/>
  <option integrator="implicitfast" timestep="0.01"/>

  <!-- ================= DEFAULT SETTINGS ================= -->
  <default>
    <default class="ur5e">
      <default class="visual">
        <geom type="mesh" contype="0" conaffinity="0" group="2"/>
      </default>
      <default class="collision">
        <geom type="capsule" group="3"/>
        <default class="eef_collision">
          <geom type="cylinder"/>
        </default>
      </default>
      <joint axis="0 1 0" range="-6.28319 6.28319"/>
      <default class="joint_limited">
        <joint range="-3.1415 3.1415"/>
      </default>
      <default class="position">
        <position ctrlrange="-6.2831 6.2831" kp="2000" kv="100" forcerange="-150 150"/>
        <default class="position_limited">
          <position ctrlrange="-3.1415 3.1415"/>
        </default>
        <default class="position_small">
          <position kp="500" kv="25" forcerange="-28 28"/>
        </default>
      </default>
    </default>
    <default class="dust">
      <geom type="mesh" density="800" friction="0.9 0.3 0.1" condim="3"/>
    </default>
  </default>

  <!-- ================= MATERIALS + MESHES ================= -->
